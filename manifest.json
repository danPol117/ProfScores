{
  "manifest_version": 3,
  "name": "ProfScores",
  "description": "Making registration easier",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "scripting"],
  "host_permissions": ["*://*.ratemyprofessors.com/*"],
  "content_scripts": [
    {
      "matches": ["https://generalssb-prod.ec.njit.edu/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
